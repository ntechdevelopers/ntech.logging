{
    "Logging": {
        "ConsoleEnabled": true,
        "Seq": {
            "Enabled": true,
            "Url": "http://localhost:5340/",
            "ApiKey": ""
        },
        "ELK": {
            "Enabled": true,
            "Url": "http://localhost:9200"
        },
        "File": {
            "Enabled": true,
            "PathFile": ".\\logs\\log.txt"
        },
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Default": "Information",
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        }
    },
    "AllowedHosts": "*"
}
